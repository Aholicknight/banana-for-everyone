{
  "schemaVersion": 1,
  "id": "bananaplus",
  "version": "${version}",
  "name": "Banana+",
  "description": "Banana+ addon for the Meteor Addon API.",
  "authors": [
    "Bennooo",
    "Necro",
    "RickyTheRacc"
  ],
  "contact": {
    "repo": "https://github.com/Bennooo/banana-for-everyone"
  },
  "icon": "assets/bananaplus/logo.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "bananaplus.modules.BananaPlus"
    ]
  },
  "mixins": [
    "meteor.mixins.json",
    "minecraft.mixins.json"
  ],
  "accessWidener": "accesswidener",
  "custom": {
    "meteor-client:color": "255,193,0"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=1.18",
    "meteor-client": ">=0.4.6"
  }
}
