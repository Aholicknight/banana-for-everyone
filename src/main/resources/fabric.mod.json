{
  "schemaVersion": 1,
  "id": "bananaplus",
  "version": "${version}",
  "name": "Banana+",
  "description": "Based PVP addon for Meteor Client.",
  "authors": [
    "Bennooo",
    "Necro",
    "RickyTheRacc"
  ],
  "contact": {
    "repo": "https://github.com/Bennooo/banana-for-everyone"
  },
  "icon": "assets/logo1.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "bananaplus.BananaPlus"
    ]
  },
  "mixins": [
    "meteorclient.mixins.json",
    "minecraft.mixins.json"
  ],
  "accessWidener": "accesswidener",
  "custom": {
    "meteor-client:color": "255,193,0"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=1.19.1",
    "meteor-client": "*"
  },
  "breaks": {
    "bedtrap": "*",
    "splashhack": "*"
  }
}
