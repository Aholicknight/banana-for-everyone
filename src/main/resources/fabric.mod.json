{
  "schemaVersion": 1,
  "id": "bananaplus",
  "version": "${version}",
  "name": "Banana+",
  "description": "Banana+ addon for the Meteor Addon API.",
  "authors": [
    "Bennooo",
    "Necro"
  ],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/template/logo.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "bplusdevelopment.modules.AddModule"
    ]
  },
  "mixins": [
    "banana.mixins.json"
  ],
  "accessWidener": "accesswidener",
  "custom": {
    "meteor-client:color": "255,193,0"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=1.18",
    "meteor-client": ">=0.4.6"
  }
}
